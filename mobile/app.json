{
  "expo": {
    "name": "Ecoleta",
    "slug": "mobile",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f0f5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  },
  "android": {
    "package": "com.nextlevelweek.ecoleta",
    "adaptiveIcon": {
      "backgroundColor": "#f0f0f5",
      "foregroundImage": "./assets/icon.png"
    },
    "versionCode": 1.0
  }
}
